<h1>CMDB: Subnets</h1>

<table class="table table-striped table-condensed">
  <thead>
  <tr>
    <th>Name</th>
    <th>Type</th>
    <th>Description</th>
    <th>Step</th>
  </tr>
  </thead>

  <tbody>
  <% @subnets.each do |subnet| %>
  <% s = subnet[:cmdb]%>
      <tr>
        <td><%= link_to s[:name], "/cmdb/subnets/#{s[:infrid]}" %> <a href="https://r2.grenoble.hp.com/operations/direct/infra/<%= s[:infrid] %>"><span class="fa fa-database"></span></a></td>
        <td><%= s[:type] %></td>
        <td><%= s[:description] %></td>
        <td><%= s[:step] %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>
